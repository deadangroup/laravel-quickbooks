{
    "name": "deadangroup/laravel-quickbooks",
    "description": "Laravel Client for QuickBooks.",
    "keywords": [
        "quickbooks",
        "client",
        "deadangroup"
    ],
    "authors": [
        {
            "name": "Deadan Group Limited",
            "email": "james@dgl.co.ke"
        }
    ],
    "require": {
        "php": ">=7.2",
        "quickbooks/v3-php-sdk": "^6.0.0"
    },
    "autoload": {
        "psr-4": {
            "DGL\\QBO\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DGL\\QBO\\Providers\\ModuleServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/thanks": true
        }
    }
}
